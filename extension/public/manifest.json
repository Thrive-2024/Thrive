{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Thrive Chrome Extension",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "./assets/icons/16.png",
    "32": "./assets/icons/32.png",
    "48": "./assets/icons/48.png",
    "96": "./assets/icons/96.png",
    "128": "./assets/icons/128.png"
  }
}
